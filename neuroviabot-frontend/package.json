{
  "name": "neuroviabot-dashboard",
  "version": "1.0.0",
  "description": "NeuroViaBot Web Dashboard",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev --turbo -p 3001",
    "dev:backend": "cd ../neuroviabot-backend && npm run dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "clean": "rm -rf .next out node_modules"
  },
  "dependencies": {
    "next": "^15.0.3",
    "react": "^19.0.0-rc.1",
    "react-dom": "^19.0.0-rc.1",
    "axios": "^1.6.0",
    "next-auth": "^4.24.0",
    "swr": "^2.2.0",
    "@headlessui/react": "^2.0.0",
    "@heroicons/react": "^2.1.0",
    "recharts": "^2.12.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "framer-motion": "^11.0.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0-rc.1",
    "@types/react-dom": "^19.0.0-rc.1",
    "typescript": "^5.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.3",
    "concurrently": "^8.2.2",
    "sass": "^1.69.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
